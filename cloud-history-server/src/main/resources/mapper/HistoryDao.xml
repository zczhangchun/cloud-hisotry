<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhangchun.history.server.dao.HistoryDao">


  <select id="selectByUidAndItemType" resultType="com.zhangchun.history.server.dto.History">
    select gid,first_time,last_time,count from history where uid=#{uid} and itemType=#{itemType}
  </select>

</mapper>